{
  "dashboard": {
    "title": "Supreme System V5 - Trading Metrics",
    "tags": ["trading", "supreme-system", "metrics"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Latency Distribution (P95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, trading_latency_bucket)",
            "legendFormat": "P95 Latency"
          }
        ],
        "yaxes": [
          {"format": "Î¼s", "label": "Latency"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 2,
        "title": "Throughput (TPS)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "rate(trading_signals_total[1m])",
            "legendFormat": "Signals/sec"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "TPS"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      },
      {
        "id": 3,
        "title": "Active Positions",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "trading_positions_active",
            "legendFormat": "Active"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 4,
        "title": "PnL (Demo)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 18, "x": 6, "y": 8},
        "targets": [
          {
            "expr": "trading_pnl_total",
            "legendFormat": "Total PnL"
          }
        ],
        "yaxes": [
          {"format": "currencyUSD", "label": "PnL"},
          {"format": "short"}
        ],
        "xaxis": {"mode": "time"}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    }
  }
}

