# Supreme System V5 - Environment Example
# Copy this file to .env and fill in your keys. DO NOT COMMIT .env

# Trading mode: sandbox or live
TRADING_MODE=sandbox

# Symbols (comma separated)
TRADING_SYMBOLS=BTC-USDT,ETH-USDT,BNB-USDT

# Risk parameters
MAX_POSITION_SIZE=0.02
STOP_LOSS_PERCENT=0.005
TAKE_PROFIT_PERCENT=0.002
MAX_DRAWDOWN_PERCENT=0.12
MAX_DAILY_LOSS_PERCENT=0.05
LEVERAGE=2

# CoinGecko (optional for higher quota)
COINGECKO_KEY=

# CoinMarketCap
CMC_KEY=

# CryptoCompare
CRYPTOCOMPARE_KEY=

# Alpha Vantage
ALPHA_VANTAGE_KEY=

# Exchange API keys (for live trading)
OKX_API_KEY=
OKX_SECRET_KEY=
OKX_PASSPHRASE=
BINANCE_API_KEY=
BINANCE_SECRET_KEY=

# Services
REDIS_URL=redis://redis:6379/0
DATABASE_URL=postgresql://postgres:supreme_password@postgres:5432/supreme_trading
METRICS_PORT=8000
LOG_LEVEL=INFO
