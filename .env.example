# Supreme System V5 - Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env file to git!

# === API Keys ===
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_SECRET_KEY=your_binance_secret_key_here
ALPHA_VANTAGE_KEY=your_alpha_vantage_key_here

# === Database Configuration ===
DATABASE_URL=postgresql://user:password@localhost:5432/supreme_system
REDIS_URL=redis://localhost:6379/0

# === Trading Configuration ===
INITIAL_CAPITAL=10000
MAX_POSITION_SIZE=0.1
RISK_FREE_RATE=0.02

# === Logging ===
LOG_LEVEL=INFO
LOG_FILE=logs/supreme_system.log

# === Security ===
SECRET_KEY=your_secret_key_here_change_in_production
JWT_SECRET=your_jwt_secret_here

# === Monitoring ===
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000

# === Feature Flags ===
ENABLE_AI_OPTIMIZER=true
ENABLE_REAL_TIME_ANALYTICS=true
ENABLE_MONITORING_DASHBOARD=true

# === Resource Limits (Optional) ===
SUPREME_MODE=
SUPREME_CPU_LIMIT=
SUPREME_MEMORY_LIMIT=
SUPREME_PERFORMANCE_MODE=
