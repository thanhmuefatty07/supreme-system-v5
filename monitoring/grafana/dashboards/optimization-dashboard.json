          {
            "expr": "histogram_quantile(0.95, rate(strategy_latency_seconds_bucket[5m])) < 0.5",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "VIOLATION",
                    "color": "red"
                  },
                  "1": {
                    "text": "COMPLIANT",
                    "color": "green"
