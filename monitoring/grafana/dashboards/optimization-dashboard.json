{
  "dashboard": {
    "id": null,
    "title": "Supreme System V5 - Optimization Metrics Dashboard",
    "tags": ["trading", "supreme-system", "optimization", "performance"],
    "timezone": "UTC",
    "panels": [
      {
        "id": 1,
        "title": "CPU Usage (Target: <88%)",
        "type": "graph",
        "targets": [
          {
            "expr": "cpu_percent_gauge",
            "legendFormat": "{{phase}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Memory Usage (Target: <3.86GB)",
        "type": "graph",
        "targets": [
          {
            "expr": "memory_in_use_bytes / (1024*1024*1024)",
            "legendFormat": "{{phase}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "unit": "GB",
            "min": 0,
            "max": 4
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Strategy Latency P50/P95 (Target: <500ms)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(strategy_latency_seconds_bucket[5m])) * 1000",
            "legendFormat": "P50 Latency (ms)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(strategy_latency_seconds_bucket[5m])) * 1000",
            "legendFormat": "P95 Latency (ms)",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "unit": "ms",
            "min": 0,
            "max": 1000
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Indicator Update Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(indicator_update_latency_seconds_sum[5m]) / rate(indicator_update_latency_seconds_count[5m]) * 1000",
            "legendFormat": "{{indicator}} {{implementation}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "unit": "ms",
            "min": 0,
            "max": 50
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Event Skip Ratio (Target: 0.2-0.8)",
        "type": "stat",
        "targets": [
          {
            "expr": "event_skip_ratio",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 0.2
                },
                {
                  "color": "yellow",
                  "value": 0.5
                },
                {
                  "color": "green",
                  "value": 0.7
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 8,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "SLO Compliance: CPU <88%",
        "type": "stat",
        "targets": [
          {
            "expr": "cpu_percent_gauge{phase=\"final\"} < 88",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "VIOLATION",
