      {
        "id": 9,
        "title": "System Uptime SLO: 99.9%",
        "type": "stat",
        "targets": [
          {
            "expr": "(1 - (rate(up[7d]) - 1)) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 99.9,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 99.9
                },
                {
                  "color": "green",
                  "value": 99.95
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 8,
          "x": 8,
          "y": 20
        }
