{
  "dashboard": {
    "id": null,
    "title": "Supreme System V5 - Trading Performance",
    "tags": ["trading", "supreme-system-v5", "production"],
    "timezone": "UTC",
    "panels": [
      {
        "id": 1,
        "title": "System Health Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"supreme-system-v5\"}",
            "legendFormat": "System Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 3,
          "w": 4,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Response Time P95",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"supreme-system-v5\"}[5m])) * 1000",
            "legendFormat": "P95 Latency (ms)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 15
                },
                {
                  "color": "red",
                  "value": 20
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 3,
          "w": 4,
          "x": 4,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Memory Usage",
        "type": "stat",
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"supreme-system-v5\"} / 1024 / 1024",
            "legendFormat": "Memory (MB)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "MB",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 12
                },
                {
                  "color": "red",
                  "value": 15
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 3,
          "w": 4,
          "x": 8,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "CPU Usage",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(process_cpu_user_seconds_total{job=\"supreme-system-v5\"}[5m]) * 100",
            "legendFormat": "CPU %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 70
                },
                {
                  "color": "red",
                  "value": 85
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 3,
          "w": 4,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Trading Win Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "supreme_trading_win_rate * 100",
            "legendFormat": "Win Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 60
                },
                {
                  "color": "green",
                  "value": 68
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 3,
          "w": 4,
          "x": 16,
          "y": 0
        }
      },
      {
        "id": 6,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"supreme-system-v5\"}[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 3
        }
      },
      {
        "id": 7,
        "title": "Response Time Distribution",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(http_request_duration_seconds_bucket{job=\"supreme-system-v5\"}[5m])",
            "legendFormat": "{{ le }}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 3
        }
      },
      {
        "id": 8,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"supreme-system-v5\", status=~\"5..\"}[5m]) / rate(http_requests_total{job=\"supreme-system-v5\"}[5m]) * 100",
            "legendFormat": "Error Rate %"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 11
        }
      },
      {
        "id": 9,
        "title": "Memory Usage Trend",
        "type": "graph",
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"supreme-system-v5\"} / 1024 / 1024",
            "legendFormat": "Memory (MB)"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 8,
          "y": 11
        }
      },
      {
        "id": 10,
        "title": "Trading Performance",
        "type": "table",
        "targets": [
          {
            "expr": "supreme_trading_win_rate",
            "legendFormat": "Win Rate"
          },
          {
            "expr": "supreme_trading_total_pnl",
            "legendFormat": "Total PnL"
          },
          {
            "expr": "supreme_trading_sharpe_ratio",
            "legendFormat": "Sharpe Ratio"
          },
          {
            "expr": "rate(supreme_trading_trades_total[5m])",
            "legendFormat": "Trades/min"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 16,
          "y": 11
        }
      },
      {
        "id": 11,
        "title": "Kubernetes Pod Status",
        "type": "table",
        "targets": [
          {
            "expr": "kube_pod_info{namespace=\"production\", created_by_kind=\"ReplicaSet\", created_by_name=~\"supreme-system-v5-.*\"}",
            "legendFormat": "{{ pod }}"
          },
          {
            "expr": "kube_pod_status_phase{namespace=\"production\", pod=~\"supreme-system-v5-.*\"}",
            "legendFormat": "{{ phase }}"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 17
        }
      },
      {
        "id": 12,
        "title": "Redis Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "redis_keyspace_hits_total / (redis_keyspace_hits_total + redis_keyspace_misses_total) * 100",
            "legendFormat": "Cache Hit Rate %"
          },
          {
            "expr": "rate(redis_net_input_bytes_total[5m])",
            "legendFormat": "Input Bytes/sec"
          },
          {
            "expr": "rate(redis_net_output_bytes_total[5m])",
            "legendFormat": "Output Bytes/sec"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 17
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}
